[user]
    name = Bill Tucker
    email = bill.h.tucker@gmail.com
[color]
    status = auto
[core]
    excludesfile = /home/bbtucker/.gitignore
    safecrlf = true
	autocrlf = false
    editor = vim
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    bh = for-each-ref --sort=committerdate refs/heads/ --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) - %(color:red)%(objectname:short)%(color:reset) - %(contents:subject) - %(authorname) (%(color:green)%(committerdate:relative)%(color:reset))'
    type = cat-file -t
    dump = cat-file -p
[push]
    default = simple
[credential]
	helper = store
