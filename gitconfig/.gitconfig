[user]
	name = Bill Tucker
	email = bill@realgo.com
[color]
	status = auto
[core]
	excludesfile = /home/bbtucker/.gitignore
	safecrlf = true
	autocrlf = input
    editor = vim
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
